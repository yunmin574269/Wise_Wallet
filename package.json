{
  "name": "wise-wallet",
  "version": "0.1.0",
  "main": "src/main.js",
  "scripts": {
    "start": "electron src/main.js",
    "packager": "electron-packager ./ Wallet electron --platform linux --arch x64 --out ./OutApp --electron-version 2.0.8 --overwrite",
    "dist": "electron-builder --win --x64"
  },
  "build": {
    "appId": "org.shennongmin.QuickStart",
    "copyright": "Open Totally",
    "compression": "normal",
    "nsis": {
        "oneClick": true,
        "perMachine": true,
        "runAfterFinish": true
    },
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2",
    "electron-builder": "^20.38.4",
    "eslint": "^5.5.0"
  },
  "dependencies": {
    "argon2": "^0.19.3",
    "bn.js": "^4.11.8",
    "sqlite3": "^4.0.2",
    "uuid": "^3.3.2",
    "web3": "^1.0.0-beta.36"
  }
}
